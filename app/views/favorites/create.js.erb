function refreshStar() {
  let starConstant = '<%= j render partial: "/producers/favorite_star", locals: { producer: @producer } %>';
  const star = document.getElementById("favorite-star-<%= @producer.id %>")
  const mapStar = document.getElementById("favorite-star-map-<%= @producer.id %>")
  star.innerHTML = starConstant;
  mapStar.innerHTML = starConstant;
}

refreshStar()
